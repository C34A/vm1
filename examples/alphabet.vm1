set r0 65 ; 'A'
set r1 63935 ; screen start addr
set r2 65535 ; screen end + 1
set r4 91 ; 'Z' + 1
loop:
store r0 @r1 ; write to "pixel"
inc r1 1 ; next addr
inc r0 1 ; next char
jlt r0 r4 :test ; don't go past 'Z'
set r0 65
test:
jlt r1 r2 :loop ; if at end, draw and reset drawing pointer
draw
set r1 63935
jmp :loop
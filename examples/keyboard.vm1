
    set r1 63872 ; start of keyboard
    set r2 63934 ; end of keyboard
    set r3 63935 ; start of screen
    ; set r3 65535 ; end of screen
    set r0 0
loop:
    load @r1 r0 ; get this key
    inc r0 48 ; shift to number part of ascii
    store r0 @r3 ; put into framebuffer


    inc r1 1 ; next key
    inc r3 1 ; next screen char
    jlt r1 r2 :loop ; keep going
    set r1 63872 ; reset to first key
    set r3 63935 ; reset to first framebuffer char
    draw
    jmp :loop